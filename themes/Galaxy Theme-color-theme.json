		"widget.shadow": "#FFFFFF00",

		// Button  Control
		"button.background": "#311B92",
		"button.foreground": "#bbbbbb",
		"button.hoverBackground": "#2a1586af",

		// Editor Colors
		"editor.background": "#131418",
		"editor.foreground": "#bbbbbb",
		

		// Activity Bar
		"activityBar.background": "#49178680",
		"activityBarBadge.background": "#49178680",
		"activityBar.foreground": "#bbbbbb",
		"activityBar.border": "#39134777",

		// Side bar
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#131418",
		"sideBarSectionHeader.background" : "#3b156994",

		// Peek view Color
		"peekViewEditor.background": "",

		// Dropdown Control
